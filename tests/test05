const m = 1, n = 200;
var a, b, c, d, e;

procedure proc1;
 var a, b, c, f, g, i, j;
 
 begin 
  a := e;
  b := d;
  f := 0;
  g := 10;

  while a < b do 
   if f < g then f := f + a;
   a := a + 1;
  end
end;

procedure proc2;
 var t, v, w, x, y, z;
 
 begin w := c; w := d; 
  y := 20; 
  z:= 2000; 
  while y < z do 
   if y < w then x := x + y;
   y := y + 2; 
  end 
 end;

begin
  e := m;
  d := n;
  call proc1;
  
  c := m; 
  d := n;
  call proc2;
end.
